{
  "name": "middleware-logger",
  "version": "1.0.0",
  "description": "HTTP/HTTPS proxy middleware with logging capabilities",
  "main": "dist/index.js",
  "bin": {
    "middleware-logger": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "dev:log": "stdbuf -oL -eL ts-node src/cli.ts 2>&1 | tee middleware-logger.log",
    "dev:log-example": "stdbuf -oL -eL ts-node src/cli.ts start -p 8000 -h api.anthropic.com -r 443 --https --chat-mode --verbose 2>&1 | tee middleware-logger.log",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["proxy", "middleware", "logging", "http", "https"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "chalk": "^4.1.2"
  }
}